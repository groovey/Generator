#!/usr/bin/env php
<?php

set_time_limit(0);

use Symfony\Component\Console\Application;
use Groovey\Generator\Generator;

include __DIR__ . '/vendor/autoload.php';
$config = include __DIR__ . '/config.php';

$generator = new Generator($config);
$app       = new Application;

$app->addCommands(
        $generator->getCommands()
	);

$status = $app->run();

exit($status);